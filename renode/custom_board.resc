using sysbus

mach create

machine LoadPlatformDescription @./custom_board.repl

sysbus LoadELF @./renode_custom_board/build/renode_custom_board.elf

showAnalyzer usart3
logLevel -1 gpioPortB.greenled
logLevel -1 sysbus.i2c1
logLevel -1 sysbus.spi1

emulation RunFor "10s"

start

sleep 2

sysbus.gpioPortA.UserButton PressAndRelease

sleep 2

sysbus.gpioPortA.UserButton PressAndRelease

quit
